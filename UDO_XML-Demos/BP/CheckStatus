SUBROUTINE CheckStatus(status)
$INCLUDE INCLUDE UDO.H
    IF status <> UDO_SUCCESS THEN
        IF status = -1 THEN
        	stat = UDOGetLastError(errcode, errmsg)
       		PRINT "ERRCODE: ":errcode
        	PRINT "ERRMSG: ":errmsg
        END ELSE
            PRINT "EDO_INVALIDHANDLE"
        END
    END
RETURN
