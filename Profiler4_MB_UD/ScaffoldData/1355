READ    14
READ 2 1 4 #  1 ID Match  1x
100


T








FileName = "File":TEST_ID
call CreateFile(Failure,Msg,FileName,0,1,4,"32BIT")
if Failure # 0 then goto ErrorExit
call OpenFile(Failure,Msg,fFileName,FileName)
if Failure # 0 then goto ErrorExit
* No match between read ID and any in the group
IdChar  = "ABCDEFGHIJKLMNOPQRSTUVWXY"; * With 'Z' removed
IdChar := "abcdefghijklmnopqrstuvwxyz"
Record = spaces(0)
RecordCount = 1
BaseId = "Z"
for Ndx1 = len(IdChar) to 1 step -1 while RecordCount > 1
	for Ndx2 = len(IdChar) to 1 step -1 while RecordCount > 1
		RecordCount -= 1
		RecordId = BaseId:IdChar[Ndx1,1]:IdChar[Ndx2,1]
		write Record on fFileName,RecordId on error
			Failure = 1
			Msg = "Cannot WRITE ":FileName:",":RecordId
			goto ErrorExit
		end
	next Ndx2
next Ndx1
RecordId = BaseId:"0"
write Record on fFileName,RecordId on error
	Failure = 1
	Msg = "Cannot WRITE ":FileName:",":RecordId
	goto ErrorExit
end
:
read Record from fFileName,RecordId else
	Failure = 1
	Msg = "Cannot READ ":FileName:",":RecordId
	goto ErrorExit
end
:
close fFileName
call DeleteFile(Failure,Msg,FileName)
