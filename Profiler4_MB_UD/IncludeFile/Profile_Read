* $options DEFAULT

ReadProfile:
************
Failure = 0
if len(ProfileId) = 0 then
	Failure = FAILURE_EMPTY_STRING_ID
	print "Empty string Id for ":ProfileName
	debug
end
read ProfileRecord from fProfile,ProfileId on error
	Failure = FAILURE_READ
	Found = 0
	StatusIO = status()
	Msg = "Cannot READ ":ProfileName:",":ProfileId
	Msg := "; error ":StatusIO
end then
	Found = 1
end else
	Found = 0
	StatusIO = status()
	if StatusIO = 0 then
		Msg = ProfileName:",":ProfileId:" non-existent"
	end else
		Failure = FAILURE_READ
		Msg = "Cannot READ ":ProfileName:",":ProfileId
		Msg := "; error ":StatusIO
	end

end
return

