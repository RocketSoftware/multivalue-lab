* $options DEFAULT

ReaduProfile:
*************
Failure = 0
if len(ProfileId) = 0 then
	Failure = FAILURE_EMPTY_STRING_ID
	print "Empty string Id for ":ProfileName
	debug
end
readu ProfileRecord from fProfile,ProfileId on error
	Failure = FAILURE_ON_ERROR
	Found = 0
	StatusIO = status()
	Msg = "Cannot READU ":ProfileName:",":ProfileId
	Msg := "; error ":StatusIO
end locked
	Failure = FAILURE_LOCKED
	Found = 0
	Msg = "Cannot READU ":ProfileName:",":ProfileId
	Msg := "; locked by user ":status()
end then
	Found = 1
end else
	Found = 0
	StatusIO = status()
	if StatusIO = 0 then
		Msg = ProfileName:",":ProfileId:" non-existent"
	end else
		Failure = FAILURE_READU
		Msg = "Cannot READU ":ProfileName:",":ProfileId
	end
end
return

