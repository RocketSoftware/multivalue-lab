* FileEquates

equ FAILURE_OPEN            to  1
equ FAILURE_LOCKED          to  2
equ FAILURE_ON_ERROR        to  3
equ FAILURE_READ            to  4
equ FAILURE_READL           to  5
equ FAILURE_READU           to  6
equ FAILURE_READV           to  7
equ FAILURE_READVL          to  8
equ FAILURE_READVU          to  9
equ FAILURE_WRITE           to 10
equ FAILURE_WRITEU          to 11
equ FAILURE_WRITEV          to 12
equ FAILURE_WRITEVU         to 13
equ FAILURE_DELETE          to 14
equ FAILURE_DELETEU         to 15
equ FAILURE_RELEASE         to 16
equ FAILURE_EMPTY_STRING_ID to 17

equ FAILURE_BAD_SEQ_FILE    to 18
equ FAILURE_TRANSIENT_LOG_FILE to 19
